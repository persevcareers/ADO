trigger:
- main

pool:
  name: default


variables:
  nginx_version: '2.4.8'
  deploy: 'dev'

jobs:
- job: UserDefinedVariables
  steps:
  - script: |
      echo "##vso[task.setvariable variable=deploy]test"
      echo "nginx version : $(nginx_version)"
      echo "deploying to : $(deploy)"
    displayName: 'utilizing variables'
